option solver './cplex';
option cplex_options
    'timing = 1'
    'mipdisplay = 2'
    'mipinterval = 1';

model task2_mod.mod;
data task2_data.dat; 

option log_file 'task2_100.log';
option show_stats 1;

solve;

print Total_swaps > task2_100.log;

print {i in (1 .. total_pairs)}: {j in (1 .. total_pairs)} pair_swap[i, j] > task2_100.out;
